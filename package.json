{
  "name": "webflow-util",
  "version": "5.0.0",
  "description": "A utility library for websites designed & hosted on Webflow",
  "homepage": "https://attr.sygnal.com",
  "license": "MIT",
  "repository": "http://github.com/sygnaltech/webflow-util",
  "keywords": [
    "webflow"
  ],
  "author": {
    "name": "Michael Wells",
    "email": "mike@sygnal.com",
    "url": "https://www.sygnal.com"
  },
  "scripts": {
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "serve -p 4000",
    "tsbuild": "tsc --watch",
    "build": "esbuild ./src/**/*.ts ./src/*.ts --bundle --sourcemap --outdir=dist --external:custom-typings/* --target=es2020",
    "watch": "esbuild ./src/**/*.ts ./src/*.ts --bundle --sourcemap --outdir=dist --watch --external:custom-typings/* --target=es2020",
    "format": "prettier src/*.ts -w"
  },
  "devDependencies": {
    "@types/youtube": "^0.0.47",
    "esbuild": "^0.15.14",
    "prettier": "^2.7.1",
    "rimraf": "^5.0.5",
    "serve": "^14.1.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@types/waypoints": "^4.0.4",
    "@vimeo/player": "^2.20.1",
    "countup.js": "github:inorganik/countUp.js",
    "dayjs": "^1.11.10",
    "gsap": "^3.12.5",
    "handlebars": "^4.7.8",
    "hotkeys-js": "^3.11.2",
    "luxon": "^3.4.4",
    "moment": "^2.29.4",
    "three": "^0.165.0",
    "typescript-cookie": "^1.0.6"
  }
}
